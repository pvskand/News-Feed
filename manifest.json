{
"manifest_version": 2,

  "name": "News Feed",
  "description": "A quick peek to what is going around the world. ",
  "version": "0.1.0",

  "permissions": [
    "http://node-hnapi.herokuapp.com/",
    "tabs"
  ],
  "short_name": "News Feed",
  "icons": { "16": "icons/icon16.png",
              "32": "icons/icon32.png",
           "48": "icons/icon48.png",
          "128": "icons/icon128.png" },
  "browser_action": {
    "default_icon": "icons/icon32.png",
    "default_popup": "popup.html"
  }
}